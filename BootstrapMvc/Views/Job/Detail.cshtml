@{
    Layout = "~/Views/Shared/_LayoutBell.cshtml";
    ViewBag.Title = "Detail";
}
<style>
    .bl_row_info label {
        font-size: 14px;
    }

    .bl_row_info .label {
        font-weight: normal;
        font-size: 18px;
    }
</style>
@section BellTop {
    <div class="panel panel-default">
        <div class="panel-body" style="background-color: rgb(0, 102, 164)">
            <div class="container">
                <div class="row">
                    <div class="col-md-7 font30">
                        <span class="label">Job List > Job #23422</span>
                    </div>
                    <div class="col-md-5">
                        <button type="button" class="btn btn-default font18">
                            <span class="glyphicon glyphicon-pencil"></span> Edit Job
                        </button>
                    </div>
                </div>
            </div>
        </div>
        <div style="height: 4px; background-color: darkred"></div>
    </div>
}
<div class="row">
    <div class="col-md-9">
        <div class="row bl_row_info">
            <div class="col-md-3">
                <div class="panel panel-default">
                    <div class="panel-body">
                        <label>Status</label>

                        <span class="label label-danger center-block">Behind</span>

                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="panel panel-default">
                    <div class="panel-body">
                        <label>Estimated Close Time</label>

                        <span class="label label-default center-block">Fri 03/14 - 6:35p</span>

                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="panel panel-default">
                    <div class="panel-body">
                        <label>Time Spent</label>

                        <span class="label label-default center-block">1hr 25min</span>

                    </div>
                </div>
            </div>
