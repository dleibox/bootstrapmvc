                    <li><a href="#profile" role="tab" data-toggle="tab">Profile</a></li>
                    <li><a href="#correspondance" role="tab" data-toggle="tab">Correspondance</a></li>
                    <li><a href="#request" role="tab" data-toggle="tab">Original Request</a></li>
                </ul>

                <!-- Tab panes -->
                <div class="tab-content">
                    <div class="tab-pane active" id="details">
